{
    "configurations": [
        {
            "name": "mm: linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:vs_prefix_prj}/include/**",
                "${env:vs_prefix_usr}/include/**",
                "/usr/lib/x86_64-linux-gnu/openmpi/include/**",
                "/usr/include/hdf5/openmpi/**",
                "/usr/include/python3.12/**"
            ],
            "defines": [
                "PYRE_CORE",
                "WITH_JOURNAL"
            ],
            "compilerPath": "/usr/bin/${env:vs_compiler}",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "intelliSenseMode": "linux-gcc-x64"
        },
        {
            "name": "mm: mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:vs_prefix_prj}/include/**",
                "${env:vs_prefix_usr}/include/**",
                "${env:vs_prefix_sys}/include/**",
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks/**"
            ],
            "defines": [
                "DEBUG",
                "PYRE_CORE",
                "WITH_JOURNAL"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "${env:vs_prefix_sys}/bin/${env:vs_compiler}",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}
